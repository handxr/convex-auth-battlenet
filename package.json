{
  "name": "convex-auth-battlenet",
  "version": "0.1.0",
  "description": "Battle.net OAuth provider for Convex Auth",
  "author": "Juan Hander",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build && bun run test"
  },
  "keywords": [
    "convex",
    "auth",
    "battlenet",
    "battle.net",
    "blizzard",
    "oauth",
    "authentication"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/handxr/convex-auth-battlenet"
  },
  "peerDependencies": {
    "@auth/core": ">=0.35.0",
    "@convex-dev/auth": ">=0.0.71"
  },
  "devDependencies": {
    "@auth/core": "^0.37.0",
    "@convex-dev/auth": "^0.0.80",
    "@types/bun": "^1.3.6",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3"
  }
}
